link: https://leetcode.com/problems/contains-duplicate-ii/description/

/**
 * @param {number[]} nums
 * @param {number} k
 * @return {boolean}
 */
var containsNearbyDuplicate = function(nums, k) {
   for(let i=0;i<nums.length;i++)
   {
    let j=i+k;
    if(j>=nums.length)
    j=nums.length-1;
    while( j!=i && j<nums.length && j>=0)
    {
        if(nums[i]===nums[j])
        return true;
        j--;
    }
   }
   return false;

};
