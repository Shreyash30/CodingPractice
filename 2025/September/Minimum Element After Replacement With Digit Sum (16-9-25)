link: https://leetcode.com/problems/minimum-element-after-replacement-with-digit-sum/description/

int minElement(int* nums, int numsSize) {
    int min=10000;
    for(int i=0;i<numsSize;i++)
    { int n=nums[i];
    int sum=0;
        while(n>0)
        {
         int rem=n%10;
          sum+=rem;
          n=n/10;
        }
        if(sum<min)
        min=sum;
    }
    return min;
}
