link: https://leetcode.com/problems/removing-stars-from-a-string/description/

char* removeStars(char* s) {
    int len=strlen(s);
   char* stack=(char*)malloc(sizeof(char)*(len+1));
    int top=-1;
    for(int i=0;i<len;i++)
    {
       if(s[i]=='*' && top!=-1)
       top--;
       else
       stack[++top]=s[i];
    }
    stack[top+1]='\0';
    return stack;
}
