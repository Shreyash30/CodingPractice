link: https://leetcode.com/problems/earliest-time-to-finish-one-task/description/

/**
 * @param {number[][]} tasks
 * @return {number}
 */
var earliestTime = function(tasks) {
    let min=201;
    for(let i=0;i<tasks.length;i++)
    {
        let sum=tasks[i][0]+tasks[i][1];
        if(sum<min)
        min=sum;
    }
    return min;
};
