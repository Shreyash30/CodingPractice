link: https://leetcode.com/problems/remove-duplicates-from-sorted-array/description/

/**
 * @param {number[]} nums
 * @return {number}
 */
var removeDuplicates = function(nums) {
    let k=nums.length;
    for(let i=0;i<k;i++)
    {
        let j=i+1;
        while(j<k && nums[i]==nums[j])
        {
            nums.push(nums.splice(j,1)[0]);
            k--;
        }
    }
    return k;
};
