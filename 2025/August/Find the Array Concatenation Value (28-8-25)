link: https://leetcode.com/problems/find-the-array-concatenation-value/description/

/**
 * @param {number[]} nums
 * @return {number}
 */
var findTheArrayConcVal = function(nums) {
    let left=0,sum=0;
    let right=nums.length-1;
    while(left<=right)
    { if(left==right)
        { sum+=nums[left];}
        else{
      let num=Number(nums[left].toString() + nums[right].toString());
      sum+=num;
    }
    left++;right--;
    }
    return sum;
};
