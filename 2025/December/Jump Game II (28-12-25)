link: https://leetcode.com/problems/jump-game-ii/description/


class Solution {
public:
    int jump(vector<int>& nums) {

    int jump=0,maxidx=0,currend=0;
        
    for(int i=0 ; i<nums.size()-1; i++)
        {   
         maxidx = max(maxidx , nums[i]+i);
         if(i == currend) 
         { 
           currend= maxidx;
           jump++;
         }
        }
        return jump;
    }
};
