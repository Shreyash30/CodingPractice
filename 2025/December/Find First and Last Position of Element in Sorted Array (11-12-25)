link: https://leetcode.com/problems/find-first-and-last-position-of-element-in-sorted-array/description/

class Solution {
public:
      vector<int> res;

      void first(vector<int>& nums,int target)
      { int temp=-1;
        int start=0;
        int end=nums.size()-1;
        while(start<=end)
        {
            int mid=start+(end-start)/2;
            if(nums[mid]==target)
            {
               temp=mid;
               end=mid-1;
            }
            else if(nums[mid]>target)
            end=mid-1;
            else
            start=mid+1;
        }
        res.push_back(temp);
      }

       void last(vector<int>& nums,int target)
      { int temp=-1;
        int start=0;
        int end=nums.size()-1;
        while(start<=end)
        {
            int mid=start+(end-start)/2;
            if(nums[mid]==target)
            {
               temp=mid;
               start=mid+1;
            }
            else if(nums[mid]>target)
            end=mid-1;
            else
            start=mid+1;
        }
        res.push_back(temp);
      }
    vector<int> searchRange(vector<int>& nums, int target) {
        first(nums,target);
        last(nums,target);
        return res;
    }
};
