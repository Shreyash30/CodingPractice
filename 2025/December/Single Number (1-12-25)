link: https://leetcode.com/problems/single-number/description/

class Solution {
public:
    int singleNumber(vector<int>& nums) {
        map<int,int> mpp;
        for(auto it:nums)
        {
            mpp[it]+=1;
        }
      for( auto it:mpp)
        {
            if(it.second==1)
            return it.first;
        }
        return 1;
    }
};
