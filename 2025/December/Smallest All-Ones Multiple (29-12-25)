link: https://leetcode.com/problems/smallest-all-ones-multiple/description/


/**
 * @param {number} k
 * @return {number}
 */
var minAllOneMultiple = function(k) {
     if (k==1) return 1;
     if (k%2 == 0 || k%5 == 0) return -1;

        let rem=0,count=0;

        do
        {
           rem = (rem*10 + 1) % k;
           count++;
        } 
        while(rem!=0 && count < k)
       
       return count;
    }
