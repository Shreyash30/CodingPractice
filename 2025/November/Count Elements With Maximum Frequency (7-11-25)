link: https://leetcode.com/problems/count-elements-with-maximum-frequency/description/

class Solution {
public:
    int maxFrequencyElements(vector<int>& nums) {
        unordered_map<int,int> mpp;
        int count=0;
        for(int i=0;i<nums.size();i++)
        {
            mpp[nums[i]]++;
        }
        int maxfreq=0;
        for(auto it: mpp)
        {
            if(it.second>maxfreq)
            maxfreq=it.second;
        }
        for(auto it: mpp)
        {
            if(it.second==maxfreq)
            count=count+maxfreq;
        }
        return count;
    }
};
