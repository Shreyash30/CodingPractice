link: https://leetcode.com/problems/minimum-string-length-after-balanced-removals/description/


class Solution {
public:
    int minLengthAfterRemovals(string s) {
        stack<char> st;
        for(int i=0;i<s.size();i++)
        {    
          if(st.empty()==false && (st.top()!=s[i]))
            st.pop();
            else
            st.push(s[i]);
        }
        return st.size();
    }
};
