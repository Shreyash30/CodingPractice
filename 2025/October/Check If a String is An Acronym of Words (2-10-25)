link: https://leetcode.com/problems/check-if-a-string-is-an-acronym-of-words/description/

/**
 * @param {string[]} words
 * @param {string} s
 * @return {boolean}
 */
var isAcronym = function(words, s) {
    let result="";
    for(let i of words)
    {
      result+=i[0];
    }
    if(result===s)
    return true;
    else
    return false;
};
