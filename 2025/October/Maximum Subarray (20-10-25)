link: https://leetcode.com/problems/maximum-subarray/description/

/**
 * @param {number[]} nums
 * @return {number}
 */
var maxSubArray = function(nums) {
    let max=nums[0],res=nums[0];
    for(let i=1;i<nums.length;i++)
    {
        max=((max+nums[i])>nums[i])?max+nums[i]:nums[i];
        res=res>max? res:max; 
    }
    return res;
};
