link: https://leetcode.com/problems/sum-of-variable-length-subarrays/description/

int subarraySum(int* nums, int numsSize) 
{ int total=0,sum=0;
    for(int i=0;i<numsSize;i++)
    {   sum=0;
        int start = 0 > (i-nums[i]) ? 0 : i-nums[i];
        for(int j=start;j<=i;j++)
        {
        sum+=nums[j];
        } 
        total+=sum;
    }
    return total;
}
